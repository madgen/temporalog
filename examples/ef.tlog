.decl v(int,int).
v(1,2).
v(2,3).
v(2,4).

.decl q(int) @ v.
q(1000) @ v 2.
q(2000) @ v 3.

?- EF v q(X).       % T = 1, X = 1000; T = 1, X = 2000
                    % T = 2, X = 1000; T = 2, X = 2000
                    %                  T = 3, X = 2000
?- EF v q(2000).    % T = 1, X = 2000; T = 2, X = 2000; T = 3, X = 2000
?- EF v EX v q(X).  % T = 1, X = 1000
                    % T = 1, X = 2000; T = 2, X = 2000
?- EF v EF v q(X).  % Same as EF v q(X)
